#!/Users/taken/Documents/python/生成AIエンジニア_Lesson23_サンプルアプリ2/ダウンロード用/Slack連携なし/customer-contact/customer-contact/new_env/bin/python3.11

import docx2txt

if __name__ == '__main__':
    import sys
    args = docx2txt.process_args()
    text = docx2txt.process(args.docx, args.img_dir)
    output = getattr(sys.stdout, 'buffer', sys.stdout)
    output.write(text.encode('utf-8'))
